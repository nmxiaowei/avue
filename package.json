  "unpkg": "lib/avue.min.js",
  "jsdelivr": "lib/avue.min.js",
  "scripts": {
    "bootstrap": "yarn --registry https://registry.npm.taobao.org || npm install --registry https://registry.npm.taobao.org && yarn gen:version",
